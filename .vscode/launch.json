    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--save_path=data/",
                "--dataset_name=tortuous_test",
                "--num_trees=10",
                "--num_branches=3",
                "--vessel_type=RCA",
                "--shear",
                "--save_visualization",
                "--generate_projections",
                "--control_point_path=./RCA_branch_control_points/tortuous"
            ]
        }
    ]
}